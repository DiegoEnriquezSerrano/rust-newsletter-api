# ## Base URL where the application is reachable (include scheme). Used to build absolute links.
# APP_APPLICATION__BASE_URL="http://127.0.0.1"

# ## 256-bit (32 byte) Alphanumeric signing key for AES encryption.
# APP_APPLICATION__CAPTCHA_SECRET="A32ByteLongAlphanumericSecretKey"

# ## HMAC secret used to sign/verify messages, cookies, tokens, or webhooks.
# ## Use a long, random, high-entropy string in production.
# APP_APPLICATION__HMAC_SECRET="super-long-and-secret-random-key-needed-to-verify-message-integrity"

# ## Host/interface the application binds to (IP or hostname).
# APP_APPLICATION__HOST=127.0.0.1

# ## Port the application server listens on.
# APP_APPLICATION__PORT=8000

# ## Name for the application session cookie.
# APP_APPLICATION__SESSION_KEY="newsletter_api_key"

# ## API key for cloudinary account.
# APP_CLOUDINARY_CLIENT__API_KEY="CHANGE_ME"

# ## API secret for cloudinary account associated with api key.
# APP_CLOUDINARY_CLIENT__API_SECRET="CHANGE_ME"

# ## Base URL for cloudinary server.
# APP_CLOUDINARY_CLIENT__BASE_URL="https://api.cloudinary.com"

# ## Bucket name associated with cloudinary account.
# APP_CLOUDINARY_CLIENT__BUCKET="CHANGE_ME"

# ## Cloudinary account ID number.
# APP_CLOUDINARY_CLIENT__ID="CHANGE_ME"

# ## Timeout treshold for cloudinary requests.
# APP_CLOUDINARY_CLIENT__TIMEOUT_MILLISECONDS=30000

# ## Name of the Postgres database the app connects to.
# APP_DATABASE__DATABASE_NAME="newsletter"

# ## Database server host or IP address.
# APP_DATABASE__HOST="127.0.0.1"

# ## Password for the database user.
# APP_DATABASE__PASSWORD="password"

# ## Port the database server listens on (default Postgres port is 5432).
# APP_DATABASE__PORT=5432

# ## Whether SSL/TLS is required for database connections (true/false).
# APP_DATABASE__REQUIRE_SSL=false

# ## Username used for client to authenticate to the database.
# APP_DATABASE__USERNAME="postgres"

# ## API key or token to authenticate with the email provider.
# APP_EMAIL_CLIENT__AUTHORIZATION_TOKEN="my-secret-token"

# ## Base URL or hostname of the email provider API.
# APP_EMAIL_CLIENT__BASE_URL="localhost"

# ## Default "from" email address for outgoing messages.
# APP_EMAIL_CLIENT__SENDER_EMAIL="test@gmail.com"

# ## Timeout for email provider requests in milliseconds.
# APP_EMAIL_CLIENT__TIMEOUT_MILLISECONDS=10000

# ## Base URL or hostname of the primary client application.
# APP_HOSTS__CLIENT="http://localhost:5173"

# ## Redis connection URI used for caching, job queues, sessions, etc.
# APP_REDIS_URI="redis://127.0.0.1:6379"

# ## Access key for primary S3 service user.
# APP_S3_CLIENT__ACCESS_KEY="CHANGEME"

# ## Base URL or hostname of S3 service.
# APP_S3_CLIENT__ENDPOINT="http://localhost:9000"

# ## Region name for S3 service.
# APP_S3_CLIENT__REGION="global"

# ## Secret key for primary S3 service user.
# APP_S3_CLIENT__SECRET_KEY="CHANGEME"


## Full Postgres connection string used by some libraries/drivers (sqlx).
DATABASE_URL="postgres://postgres:password@localhost:5432/newsletter"


## Postgres listening port.
PGPORT=5432

## Database name for Postgres container/environment.
POSTGRES_DB=newsletter

## Host for Postgres container/environment.
POSTGRES_HOST=localhost

## Password for Postgres container/environment.
POSTGRES_PASSWORD=password

## Username for Postgres container/environment.
POSTGRES_USER=postgres


## Primary Access key for RustFS service.
RUSTFS_ACCESS_KEY="CHANGEME"

## Secret key for primary RustFS account.
RUSTFS_SECRET_KEY="CHANGEME"

## Region for RustFS service.
RUSTFS_REGION="global"

## Host for RustFS API service.
RUSTFS_ADDRESS="0.0.0.0:9000"

## Host for RustFS client service.
RUSTFS_CONSOLE_ADDRESS="0.0.0.0:9001"

## Flag to enable or disable RustFS client.
RUSTFS_CONSOLE_ENABLE=true

## CORS policy for RustFS client.
RUSTFS_CONSOLE_CORS_ALLOWED_ORIGINS="*"

## CORS policy for RustFS server.
RUSTFS_CORS_ALLOWED_ORIGINS="*"
